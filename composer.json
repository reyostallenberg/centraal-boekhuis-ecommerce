{
    "name": "reyostallenberg/centraal-boekhuis-ecommerce",
    "description": "E-commerce utility library for Centraal Boekhuis",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Reyo Stallenberg",
            "email": "reyostallenberg@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2",
        "symfony/console": "^3.4 || ^4.3",
        "symfony/validator": "^3.4 || ^4.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.3",
        "gorhill/php-fine-diff": "^0.1.0",
        "phpunit/phpunit": "^5.7",
        "symfony/var-dumper": "^3.4 || ^4.3"
    },
    "autoload": {
        "psr-4": {"Reyostallenberg\\CentraalBoekhuisEcommerce\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"Reyostallenberg\\CentraalBoekhuisEcommerce\\Tests\\": "tests/"}
    },
    "config": {
        "sort-packages": true
    }
}
